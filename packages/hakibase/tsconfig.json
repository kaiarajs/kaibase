{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "resolveJsonModule": true,    
    "esModuleInterop": true,
    "allowJs": true,
    "noEmit": false, // <- remove this or set to false
    "incremental": false, // <- r
    "isolatedModules": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
